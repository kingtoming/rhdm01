package com.axa.dm.autouw;

import java.lang.Number;

rule "mapClassCode - ApplicationRider"
	dialect "mvel"
	when
		$appRider : ApplicationRider( $classCode : classCode, $planCode : planCode, standardClass == null )
		StandardRateObject( classCode == $classCode , planCode == $planCode )
	then
		modify( $appRider ) {
				setStandardClass( true )
		}
end
