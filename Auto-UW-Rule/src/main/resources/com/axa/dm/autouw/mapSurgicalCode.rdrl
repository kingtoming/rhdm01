package com.axa.dm.autouw;

import java.lang.Number;

rule "mapSurgicalCode"
	dialect "mvel"
	when
		$claim : Claim( $surgicalcodes : surgicalcodes, surgicalCodeInList == false )
		DiagnosticSurgicalObject( code == $surgicalcodes )
	then
		modify( $claim ) {
				setSurgicalCodeInList( true )
		}
end
