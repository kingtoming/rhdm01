package com.axa.dm.autouw;

import java.lang.Number;

rule "mapClassCode - Policy"
	dialect "mvel"
	when
		$policy : Policy( $planCode : planCode, $classCode : classCode, standardClass == null )
		StandardRateObject( planCode == $planCode , classCode == $classCode )
	then
		modify( $policy ) {
				setStandardClass( true )
		}
end
