package com.axa.dm.autouw;

import java.lang.Number;

rule "mapClassCode - ApplicationRider (subStandard)"
	dialect "mvel"
	when
		$appRider : ApplicationRider( $classCode : classCode, $planCode : planCode, standardClass == null )
		StandardRateObject( planCode == $planCode )
		not (StandardRateObject( classCode == $classCode , planCode == $planCode )) 
	then
		modify( $appRider ) {
				setStandardClass( false )
		}
end
