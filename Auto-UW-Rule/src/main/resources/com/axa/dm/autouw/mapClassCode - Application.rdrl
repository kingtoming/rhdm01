package com.axa.dm.autouw;

import java.lang.Number;

rule "mapClassCode - Application"
	dialect "mvel"
	when
		$app : Application( $classCode : classCode, $planCode : planCode, standardClass == null )
		StandardRateObject( classCode == $classCode , planCode == $planCode )
	then
		modify( $app ) {
				setStandardClass( true )
		}
end
